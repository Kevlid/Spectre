{
    "name": "spectre",
    "version": "5.0.0",
    "description": "",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Kewwie/Spectre.git"
    },
    "author": "Kewwie",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Kewwie/Spectre/issues"
    },
    "homepage": "https://github.com/Kewwie/Spectre#readme",
    "scripts": {
        "build": "tsc --build",
        "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
        "start": "cross-env NODE_ENV=production node -r ./register-tsconfig-paths.js dist/index.js",
        "typeorm": "ts-node-dev --transpile-only node_modules/typeorm/cli.js",
        "migration:generate": "pnpm typeorm -- migration:generate -n",
        "migration:run": "pnpm typeorm -- migration:run -d src/datasource.ts",
        "migration:revert": "pnpm typeorm -- migration:revert"
    },
    "devDependencies": {
        "@types/node": "^22.15.12",
        "cross-env": "^7.0.3",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@discordjs/builders": "^1.11.2",
        "@discordjs/rest": "^2.5.0",
        "@sentry/cli": "^2.45.0",
        "@sentry/node": "^9.18.0",
        "discord-api-types": "^0.38.11",
        "discord.js": "^14.19.3",
        "dotenv": "^16.5.0",
        "node-schedule": "^2.1.1",
        "pg": "^8.16.0",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.24"
    }
}
