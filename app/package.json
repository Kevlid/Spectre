{
  "name": "katt-app",
  "version": "3.0.0",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typeorm": "npx typeorm",
    "migrate:create": "npm run typeorm -- migration:create src/migrations/new",
    "migrate:run": "npm run typeorm -- migration:run -d dist/datasource.js",
    "migrate:revert": "npm run typeorm -- migration:revert -d dist/datasource.js"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@discordjs/voice": "^0.17.0",
    "discord-api-types": "^0.37.79",
    "discord.js": "^14.12.1",
    "dotenv": "^16.4.5",
    "mongodb": "^5.9.2",
    "mysql2": "^3.9.4",
    "node-schedule": "^2.1.1",
    "typeorm": "^0.3.20",
    "unofficial-valorant-api": "^2.3.1-stable.0"
  }
}
